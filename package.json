    "yeoman-generator",
    "jhipster-module",
    "jhipster-5",
    "pulsar",
    "messaging"
  ],
  "homepage": "https://github.com/cbornet/generator-jhipster-pulsar",
  "author": {
    "name": "Christophe Bornet",
    "email": "cbornet@hotmail.com",
    "url": "https://github.com/cbornet"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbornet/generator-jhipster-pulsar.git"
  },
  "dependencies": {
    "chalk": "3.0.0",
    "generator-jhipster": ">=5.0.0",
    "js-yaml": "3.13.1",
    "mkdirp": "1.0.3",
    "semver": "7.1.2",
    "shelljs": "0.8.3",
    "yeoman-generator": "4.5.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "fs-extra": "8.1.0",
    "mocha": "7.0.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "3.0.0"
  },
  "scripts": {
    "pretest": "eslint .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
